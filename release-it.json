{
  "github": {
    "release": true,
    "releaseName": "Release ${version}",
    "preRelease": false,
    "draft": false,
    "tokenRef": "GITHUB_TOKEN",
    "assets": "",
    "timeout": 0
  },
  "hooks": {
    "before:beforeBump": [
      "nimble version ${version}",
      "nimble docgen"
    ]
  }
}